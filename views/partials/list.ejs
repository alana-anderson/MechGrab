<!-- Render selling items :: views/partials/list.ejs -->
<ul>
    <% list.forEach(function(item) { %>
        <li><a href="<%= item.url %>"><%= item.title %></a></li>
		<% if(item.comments >= 2){ %>
              <p><%= item.comments %> comments </p>
            <% } else{ %>
            	<p>[ NEW ] <%= item.comments %> comments -- BE THE FIRST </p>
            <% } %>
    <% }); %>
</ul>